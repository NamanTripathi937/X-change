<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="-35dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView4"
        app:layout_constraintEnd_toEndOf="@+id/cardView4"
        app:layout_constraintStart_toStartOf="@+id/cardView4"
        app:srcCompat="@drawable/start_image" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="84dp"
        android:layout_height="83dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="-20dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView4"
        app:layout_constraintEnd_toEndOf="@+id/cardView4"
        app:layout_constraintStart_toStartOf="@+id/cardView4"
        app:srcCompat="@drawable/start_image_flip" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView4"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/reviewDialog_star1"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginEnd="10dp"
                android:backgroundTint="@color/indigo_dark"
                app:layout_constraintBottom_toBottomOf="@+id/reviewDialog_star2"
                app:layout_constraintEnd_toStartOf="@+id/reviewDialog_star2"
                app:layout_constraintTop_toTopOf="@+id/reviewDialog_star2"
                app:srcCompat="@drawable/baseline_star_border_24" />

            <ImageView
                android:id="@+id/reviewDialog_star4"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="10dp"
                android:backgroundTint="@color/indigo_dark"
                app:layout_constraintBottom_toBottomOf="@+id/reviewDialog_star3"
                app:layout_constraintStart_toEndOf="@+id/reviewDialog_star3"
                app:layout_constraintTop_toTopOf="@+id/reviewDialog_star3"
                app:srcCompat="@drawable/baseline_star_border_24" />

            <ImageView
                android:id="@+id/reviewDialog_star5"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginStart="10dp"
                android:backgroundTint="@color/indigo_dark"
                app:layout_constraintBottom_toBottomOf="@+id/reviewDialog_star4"
                app:layout_constraintStart_toEndOf="@+id/reviewDialog_star4"
                app:layout_constraintTop_toTopOf="@+id/reviewDialog_star4"
                app:srcCompat="@drawable/baseline_star_border_24" />

            <ImageView
                android:id="@+id/reviewDialog_star3"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:backgroundTint="@color/indigo_dark"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/reviewDialog_msg"
                app:srcCompat="@drawable/baseline_star_border_24" />

            <ImageView
                android:id="@+id/reviewDialog_star2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginEnd="10dp"
                android:backgroundTint="@color/indigo_dark"
                app:layout_constraintBottom_toBottomOf="@+id/reviewDialog_star3"
                app:layout_constraintEnd_toStartOf="@+id/reviewDialog_star3"
                app:layout_constraintTop_toTopOf="@+id/reviewDialog_star3"
                app:srcCompat="@drawable/baseline_star_border_24" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="30dp"
                android:text="Give Review"
                android:textSize="30dp"
                android:textStyle="bold|italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/reviewDialog_msg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="30dp"
                android:ems="10"
                android:hint="your review"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView5"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <Button
                android:id="@+id/reviewDialog_cancelBtn"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="15dp"
                android:text="cancel"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/reviewDialog_msg"
                app:layout_constraintTop_toBottomOf="@+id/reviewDialog_star3" />

            <Button
                android:id="@+id/reviewDialog_doneBtn"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="15dp"
                android:text="done"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/reviewDialog_msg"
                app:layout_constraintTop_toBottomOf="@+id/reviewDialog_star3" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="84dp"
                android:layout_height="83dp"
                android:layout_marginEnd="90dp"
                android:layout_marginBottom="-20dp"
                app:layout_constraintBottom_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/start_image_flip" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>